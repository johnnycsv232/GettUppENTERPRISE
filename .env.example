# GettUpp Enterprise - Environment Variables Template
# Copy this file to .env.local and fill in the values
# NEVER commit .env.local to version control

# ============================================
# Firebase Client SDK
# ============================================
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# ============================================
# Firebase Admin SDK
# ============================================
FIREBASE_ADMIN_PROJECT_ID=your_project_id
FIREBASE_ADMIN_CLIENT_EMAIL=your_service_account_email
FIREBASE_ADMIN_PRIVATE_KEY="your_private_key"
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json
FIREBASE_PROJECT_ID=your_project_id

# ============================================
# Stripe
# ============================================
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRICE_PILOT=price_your_pilot_price_id
STRIPE_PRICE_T1=price_your_t1_price_id
STRIPE_PRICE_T2=price_your_t2_price_id
STRIPE_PRICE_VIP=price_your_vip_price_id

# ============================================
# GOD MODE 2.0 - Turso (Edge SQLite Database)
# ============================================
TURSO_DATABASE_URL=libsql://your-database.turso.io
TURSO_AUTH_TOKEN=your_turso_auth_token
TURSO_ORG_SLUG=your_organization_slug

# ============================================
# GOD MODE 2.0 - Cloudflare Workers
# ============================================
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id

# ============================================
# GOD MODE 2.0 - Sentry (Observability)
# ============================================
NEXT_PUBLIC_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id
SENTRY_AUTH_TOKEN=your_sentry_auth_token
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project

# ============================================
# Agent API Security
# ============================================
AGENT_SECRET_KEY=your_agent_secret_key

# ============================================
# Sanity CMS
# ============================================
NEXT_PUBLIC_SANITY_PROJECT_ID=your_sanity_project_id
NEXT_PUBLIC_SANITY_DATASET=production
NEXT_PUBLIC_SANITY_API_VERSION=2024-01-01
SANITY_API_TOKEN=your_sanity_api_token
SANITY_PROJECT_ID=your_sanity_project_id
SANITY_DATASET=production
SANITY_ORG_ID=your_sanity_org_id

# ============================================
# Cal.com
# ============================================
CAL_API_KEY=cal_live_your_cal_api_key

# ============================================
# Notion
# ============================================
NOTION_API_KEY=ntn_your_notion_integration_token

# ============================================
# External APIs - MCP Servers
# ============================================
CONTEXT7_API_KEY=ctx7sk-your_context7_api_key
HUGGINGFACE_API_KEY=hf_your_huggingface_api_key
HF_TOKEN=hf_your_huggingface_api_key
GITHUB_TOKEN=ghp_your_github_personal_access_token
GITHUB_PERSONAL_ACCESS_TOKEN=ghp_your_github_personal_access_token

# ============================================
# reCAPTCHA
# ============================================
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SITE_KEY=your_recaptcha_site_key

# ============================================
# App Configuration
# ============================================
NEXT_PUBLIC_APP_URL=https://your-app-url.vercel.app
NODE_ENV=development

# ============================================
# Vercel
# ============================================
VERCEL_TOKEN=vck_your_vercel_token

# ============================================
# Upstash Redis (for rate limiting)
# ============================================
UPSTASH_REDIS_REST_URL=https://your-redis-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# ============================================
# UploadThing (File Uploads)
# ============================================
UPLOADTHING_SECRET=sk_live_your_uploadthing_secret
UPLOADTHING_APP_ID=your_uploadthing_app_id

# ============================================
# Trigger.dev (Background Jobs)
# ============================================
TRIGGER_API_KEY=tr_your_trigger_api_key
TRIGGER_API_URL=https://api.trigger.dev
