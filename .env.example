# ===========================================
# GettUpp Enterprise Environment Variables
# ===========================================
# Copy this file to .env.local and fill in values
# NEVER commit .env.local to git

# -------------------------------------------
# Firebase Client SDK (public, used in browser)
# Get from: Firebase Console > Project Settings > General
# -------------------------------------------
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# -------------------------------------------
# Firebase Admin SDK (private, server-side only)
# Get from: Firebase Console > Project Settings > Service Accounts > Generate New Private Key
# -------------------------------------------
FIREBASE_ADMIN_PROJECT_ID=
FIREBASE_ADMIN_CLIENT_EMAIL=
# Full private key including BEGIN/END markers, with \n for newlines
FIREBASE_ADMIN_PRIVATE_KEY=

# -------------------------------------------
# Stripe (private, server-side only)
# Get from: Stripe Dashboard > Developers > API Keys
# -------------------------------------------
# Starts with sk_test_ (test) or sk_live_ (production)
STRIPE_SECRET_KEY=
# Get from: Stripe Dashboard > Developers > Webhooks > Signing Secret
# Starts with whsec_
STRIPE_WEBHOOK_SECRET=

# -------------------------------------------
# Security
# -------------------------------------------
# Random string for agent API authentication
# Generate with: openssl rand -hex 32
AGENT_SECRET_KEY=

# -------------------------------------------
# Optional: Monitoring
# -------------------------------------------
# Slack webhook URL for alerts
SLACK_WEBHOOK_URL=

# -------------------------------------------
# Notion Integration (Business Hub sync)
# -------------------------------------------
# Get from: notion.so/my-integrations -> Create new integration
NOTION_API_KEY=

# -------------------------------------------
# Cal.com Integration (Scheduling)
# -------------------------------------------
# Get from: app.cal.com/settings/developer/api-keys
CAL_API_KEY=

# -------------------------------------------
# Stripe Price IDs (Production)
# -------------------------------------------
# Create products in Stripe Dashboard and add price IDs here
STRIPE_PRICE_PILOT=
STRIPE_PRICE_T1=
STRIPE_PRICE_T2=
STRIPE_PRICE_VIP=

# -------------------------------------------
# App Configuration
# -------------------------------------------
NEXT_PUBLIC_APP_URL=http://localhost:3000
